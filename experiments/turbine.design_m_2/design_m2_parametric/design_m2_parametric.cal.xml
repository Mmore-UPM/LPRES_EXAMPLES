<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<experiment id="design_m2_parametric" version="1.3.0" component="turbine" partition="design_m_2">
  <header/>
  <options>
    <maximumTimeCalculation id="maxTimeCalculation" maxTime="" active="no"/>
  </options>
  <case id="case1" noplotted="no" description="Description" run="yes" type="standard case">
    <parametric_steady id="parametric1" description="Parametric">
      <parametric_transient id="parametric1" description="Parametric">
        <settings>
          <advanced advancedTransient="false" nonLinearSolver="false"/>
        </settings>
        <general>
          <tsolver nsteps="1" tstop="" initIntegStep="" time="" cint="" reportMode="IS_STEP" maxrelerror="" maxIntegStep="" imethod="DASSL" maxabserror="" warnStepReduct="FALSE" rdigits="" tsmitg="FALSE"/>
          <nlsolver maxfval="40000" toltype="RELTOL" toleranceUser="" toleranceUsed="suggested" smethod="NR" maxbroyden="60" tolerance="" maxjaceval="60" rdigits="" maxiter="10000"/>
          <stopSimulation stopBadOperation="no" stopAlgebraicsFails="yes"/>
          <debug level=""/>
          <report id="" vars="" alphasort="TRUE" create="yes" filterEquivalents="TRUE" type="REPORT_TABLE"/>
        </general>
      </parametric_transient>
      <general>
        <nlsolver maxfval="40000" toltype="RELTOL" smethod="NR" maxbroyden="60" tolerance="1e-06" maxjaceval="60" rdigits="9" maxiter="10000"/>
        <stopSimulation stopBadOperation="no"/>
        <debug level="1"/>
        <report id="report.rpt" vars="*" alphasort="TRUE" create="yes" filterEquivalents="TRUE" type="REPORT_TABLE"/>
        <initialization value="yes"/>
        <run value="yes"/>
      </general>
      <global>
        <bounds>
          <variable id="Inlet.Tt" alias="" value="1500"/>
          <variable id="Inlet.pt" alias="" value="31*101325"/>
          <variable id="LPRES.Altitude" alias="" value="0"/>
          <variable id="Nozzle.g.W" alias="" value="50"/>
          <variable id="Turbine.m.Power" alias="" value="31e6"/>
          <variable id="Turbine.phi" alias="" value="Turbine.phi_d"/>
          <variable id="Turbine.rpm" alias="" value="12000"/>
        </bounds>
        <initial>
          <variable id="Turbine.U" maxStepAbs="1.79769313e+308" alias="" dxAbs="1.5e-08" value="500" maxStepRel="1.79769313e+308" dxRel="1.5e-08"/>
        </initial>
        <closure>
          <equation oper="=" toltype="ABSTOL" left="Turbine.psi" right="Turbine.tau / Turbine.U ** 2" internal="yes" tolerance="1e-06"/>
          <equation oper="=" toltype="ABSTOL" left="Turbine.V_z2" right="Turbine.cos_2 * Turbine.M_2 * sqrt(LPRES.gamma(Inlet.fluidV) * LPRES.R(Inlet.fluidV) * Inlet.Tt / (1 + (LPRES.gamma(Inlet.fluidV) - 1) / 2 * Turbine.M_2 ** 2))" internal="yes" tolerance="1e-06"/>
        </closure>
        <parameters>
          <parameter id="Turbine.M_2" alias="Turbine.M_2" mode="INTERVAL" submode="By Final Value">
            <feature id="final" value="1.4"/>
            <feature id="initial" value="0.8"/>
            <feature id="step" value="3"/>
          </parameter>
          <output id="Turbine.U"/>
          <output id="Turbine.M_2"/>
        </parameters>
      </global>
    </parametric_steady>
  </case>
</experiment>

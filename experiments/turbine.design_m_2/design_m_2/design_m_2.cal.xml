<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<experiment id="design_m_2" version="1.3.0" component="turbine" partition="design_m_2">
  <header/>
  <options>
    <maximumTimeCalculation id="maxTimeCalculation" maxTime="" active="no"/>
  </options>
  <case id="case1" noplotted="no" description="Description" run="yes" type="standard case">
    <steady id="steady1" description="Steady">
      <general>
        <caltype/>
        <nlsolver maxfval="40000" toltype="RELTOL" smethod="NR" maxbroyden="60" tolerance="1e-06" maxjaceval="60" rdigits="9" maxiter="10000"/>
        <stopSimulation stopBadOperation="no"/>
        <debug level="1"/>
        <report id="report.rpt" vars="*" alphasort="TRUE" create="yes" filterEquivalents="TRUE" type="SAVE_VARS"/>
        <initialization value="yes"/>
        <run value="yes"/>
      </general>
      <global>
        <bounds>
          <variable id="Inlet.Tt" alias="" value="1500"/>
          <variable id="Inlet.pt" alias="" value="101325*30"/>
          <variable id="LPRES.Altitude" alias="" value="0"/>
          <variable id="Nozzle.g.W" alias="" value="40"/>
          <variable id="Turbine.m.Power" alias="" value="30e6"/>
          <variable id="Turbine.phi" alias="" value="Turbine.phi_d"/>
          <variable id="Turbine.rpm" alias="" value="25000"/>
        </bounds>
        <initial>
          <variable id="Turbine.U" maxStepAbs="1.79769313e+308" alias="" dxAbs="1.5e-08" value="500" maxStepRel="1.79769313e+308" dxRel="1.5e-08"/>
          <variable id="Turbine.M_2" maxStepAbs="1.79769313e+308" alias="" dxAbs="1.5e-08" value="0.1" maxStepRel="1.79769313e+308" dxRel="1.5e-08"/>
        </initial>
        <closure>
          <equation oper="=" toltype="ABSTOL" left="Turbine.psi" right="Turbine.tau / Turbine.U ** 2" internal="yes" tolerance="1e-06"/>
          <equation oper="=" toltype="ABSTOL" left="Turbine.V_z2" right="Turbine.cos_2 * Turbine.M_2 * sqrt(LPRES.gamma(Inlet.fluidV) * LPRES.R(Inlet.fluidV) * Inlet.Tt / (1 + (LPRES.gamma(Inlet.fluidV) - 1) / 2 * Turbine.M_2 ** 2))" internal="yes" tolerance="1e-06"/>
        </closure>
      </global>
    </steady>
  </case>
</experiment>

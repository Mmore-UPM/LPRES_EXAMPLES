<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<experiment id="mapa" version="1.3.0" component="Turbine_test" partition="off_design">
  <header/>
  <options>
    <maximumTimeCalculation id="maxTimeCalculation" maxTime="" active="no"/>
  </options>
  <case id="case1" noplotted="no" description="Description" run="yes" type="standard case">
    <restoreState id="restoreState1" description="Restore State">
      <general>
        <run value="yes"/>
        <initialization value="yes"/>
        <restore format="T_TEXT" filename="@LPRES_EXAMPLES@/experiments/+turbine_test.partition1/exp1/report.rpt.xml" filterEquiv="TRUE" category="" type="RESTORE_STATE"/>
      </general>
    </restoreState>
    <parametric_steady id="parametric2" description="Parametric">
      <parametric_transient id="parametric2" description="Parametric">
        <settings>
          <advanced advancedTransient="false" nonLinearSolver="false"/>
        </settings>
        <general>
          <tsolver nsteps="1" tstop="" initIntegStep="" time="" cint="" reportMode="IS_STEP" maxrelerror="" maxIntegStep="" imethod="DASSL" maxabserror="" warnStepReduct="FALSE" rdigits="" tsmitg="FALSE"/>
          <nlsolver maxfval="40000" toltype="RELTOL" toleranceUser="" toleranceUsed="suggested" smethod="NR" maxbroyden="60" tolerance="" maxjaceval="60" rdigits="" maxiter="10000"/>
          <stopSimulation stopBadOperation="no" stopAlgebraicsFails="yes"/>
          <debug level=""/>
          <report id="" vars="" alphasort="TRUE" create="yes" filterEquivalents="TRUE" type="REPORT_TABLE"/>
        </general>
      </parametric_transient>
      <general>
        <nlsolver maxfval="40000" toltype="RELTOL" smethod="NR" maxbroyden="60" tolerance="1e-06" maxjaceval="60" rdigits="9" maxiter="10000"/>
        <stopSimulation stopBadOperation="no"/>
        <debug level="4"/>
        <report id="report.rpt" vars="*" alphasort="TRUE" create="no" filterEquivalents="TRUE" type="REPORT_TABLE"/>
        <initialization value="no"/>
        <run value="yes"/>
      </general>
      <global>
        <bounds>
          <variable id="Inlet.Tt" alias="" value="1500"/>
          <variable id="Inlet.pt" alias="" value="10397500"/>
          <variable id="LPRES.Altitude" alias="" value="0"/>
        </bounds>
        <initial>
          <variable id="Turbine.M_2" maxStepAbs="1.79769313e+308" alias="" dxAbs="1.5e-08" value="0.9" maxStepRel="1.79769313e+308" dxRel="1.5e-08"/>
        </initial>
        <closure>
          <equation oper="=" toltype="ABSTOL" left="Nozzle.g.W" right="Nozzle.A_sl * LPRES.FGAMMA(Inlet.fluidV) * Nozzle.g.pt / sqrt(Nozzle.g.Tt * LPRES.R(Inlet.fluidV))" internal="yes" tolerance="1e-06"/>
        </closure>
        <parameters>
          <parameter id="Turbine.rpm" alias="Turbine.rpm" mode="INTERVAL" submode="By Final Value">
            <feature id="final" value="75000"/>
            <feature id="initial" value="50000"/>
            <feature id="step" value="5"/>
          </parameter>
          <parameter id="Nozzle.A" alias="Nozzle.A" mode="INTERVAL" submode="By Final Value">
            <feature id="final" value="0.25"/>
            <feature id="initial" value="0.08"/>
            <feature id="step" value="100"/>
          </parameter>
          <output id="Turbine.M_2"/>
        </parameters>
      </global>
    </parametric_steady>
  </case>
</experiment>
